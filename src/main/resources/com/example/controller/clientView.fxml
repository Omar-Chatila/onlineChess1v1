<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="490.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/20.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controller.ClientController">
    <children>
        <Label fx:id="roleLabel" layoutX="71.0" layoutY="17.0" text="Chess - Black">
            <font>
                <Font name="Arial Rounded MT Bold" size="23"/>
            </font>
        </Label>
        <AnchorPane fx:id="chessBoardPane" layoutX="71.0" layoutY="60.0" minHeight="400.0" minWidth="400.0"
                    style="-fx-border-color: BLACK;"/>
        <StackPane fx:id="stackpane" layoutX="481.0" layoutY="60.0">
            <children>
                <VBox fx:id="tableBox" prefHeight="400.0" prefWidth="200.0"/>
            </children>
        </StackPane>
        <ToggleButton fx:id="toggleButton" layoutX="471.0" layoutY="22.0" mnemonicParsing="false" onAction="#toggle"
                      text="Chat"/>
    </children>
</AnchorPane>
